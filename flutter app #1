    return MaterialApp(
      theme: ThemeData(primaryColor: Colors.red),
      home: Scaffold(
        appBar: AppBar(
          title: Text('My App'),
          centerTitle: true,
          backgroundColor: Colors.red,
        ),
        body: Center(
          child: Text("Hello!", style: TextStyle(
          color: Colors.red,
          fontSize: 24,
          fontFamily: "Arial"
        ),
        ),
        ),
        floatingActionButton: FloatingActionButton(
          backgroundColor: Colors.red,
          child: const Icon(Icons.backspace_outlined),
          onPressed: () {},
        ),
      ),
    );
  }
}  
